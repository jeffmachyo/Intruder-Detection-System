	uint8_t name[25];
	sensorData sd3 = (sensorData*)(*arg);
//	memcpy(name,*((sensorData*)arg->sensorName),strlen((const char*)(*(arg->sensorName)))+1);

//	memcpy(name,*((sensorData*)arg->sensorName),strlen((const char*)(*(arg->sensorName)))+1);


	return true;
}
static bool decode_string(pb_ostream_t *stream, const pb_field_t *field, void * const *arg) {
	return true;
}
